<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>TIWpureHTML</display-name>
  <context-param>
    <param-name>dbUrl</param-name>
    <param-value>jdbc:mysql://localhost:3306/tiwdb?serverTimezone=UTC</param-value>
  </context-param>
  <context-param>
    <param-name>dbUser</param-name>
    <param-value>YOUR_DBMS_USER</param-value>
  </context-param>
  <context-param>
    <param-name>dbPassword</param-name>
    <param-value>YOUR_DBMS_PASSWORD</param-value>
  </context-param>
  <context-param>
    <param-name>dbDriver</param-name>
    <param-value>com.mysql.cj.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>imageDefaultPath</param-name>
    <param-value>YOUR_PATH_FOR_IMAGES</param-value>
  </context-param>
  <context-param>
    <param-name>audioDefaultPath</param-name>
    <param-value>YOUR_PATH_FOR_AUDIO_TRACKS</param-value>
  </context-param>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
